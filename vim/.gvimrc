" Colorscheme
" colorscheme ir_black_smatter
" colorscheme nucolors
"colorscheme smyck_org
"colorscheme github
colorscheme twilight

" Hide clutter
set guioptions-=T
set guioptions-=L
set guioptions-=r
set guioptions-=m

" Font settings
"set guifont=Monaco\ 11
set guifont=Source\ Code\ Pro:h14
"set guifont=Inconsolata-g:h13

" Disable autochdir in graphical vim (for projects)
set noautochdir

